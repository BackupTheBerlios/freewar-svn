#
# TABLE: phpbb2_posts_text
#
DROP TABLE IF EXISTS phpbb2_posts_text;
CREATE TABLE phpbb2_posts_text(
	post_id mediumint(8) unsigned NOT NULL,
	bbcode_uid varchar(10) NOT NULL,
	post_subject varchar(60),
	post_text text, 
	PRIMARY KEY (post_id)
);

#
